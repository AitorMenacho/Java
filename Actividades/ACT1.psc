Proceso ACT1
	
	Definir num1,num2 Como Real;
	
	Escribir "Primer número para las cuentas";
	Leer num1;
	Escribir "Segundo número para las cuentas";
	Leer num2;
	
	Escribir "Suma";
	Escribir num1," + ",num2,"=",num1+num2;
	
	Escribir "Resta";
	Escribir num1," - ",num2,"=",num1-num2;
	
	Escribir "Multiplicación";
	Escribir num1," x ",num2,"=",num1*num2;
	
FinProceso
